{
  "name": "betterqjump",
  "displayName": "betterQJump",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hop",
        "title": "BetterQJump: Hop"
      }
    ],
    "keybindings": [
      {
        "command": "hop",
        "key": "ctrl+s",
        "when": "editorTextFocus && !editorHasSelection",
        "win": "shift+s",
        "mac": "cmd+s",
        "linux": "shift+S"
      }
    ]
  },
  "activationEvents": [
    "onCommand:betterqjump.helloWorld"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
